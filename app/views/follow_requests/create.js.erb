<!--Query the follow/unfollow button with sender_id and recipient_id-->

var current = $("#sender_<%= @follow_request.sender.id %>_recipient_<%= @follow_request.recipient.id %>_follow_unfollow");

var update = $("<%= j(render "follow_requests/follow_unfollow", sender: @follow_request.sender, recipient: @follow_request.recipient) %>");

current.replaceWith(update)

console.log("You followed this user")
